<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Battery Saver Dimss -->
    <string name="battery_saver_dismiss_title">Dismiss</string>
    <string name="battery_saver_notification_action_text_short">Turn off</string>

    <!-- The text for the button in the notification window-shade that clears
         all of the currently visible notifications. [CHAR LIMIT=10]-->
    <string name="clear_recents">Close all recent</string>

    <!-- Kill app toast -->
    <string name="app_killed_message">App Killed</string>

    <!-- Screenshot -->
    <string name="shortcut_screenshot">Screenshot</string>

    <!-- Battery level for expanded quick settings [CHAR LIMIT=2] -->
    <string name="battery_level_template">
        <xliff:g id="level" example="45">%d</xliff:g>%%</string>

    <!-- Weather string format in expanded statusbar header -->
    <string name="status_bar_expanded_header_weather_format">
        <xliff:g id="temp">%1$s</xliff:g>
        -
        <xliff:g id="condition">%2$s</xliff:g>
    </string>

    <!-- Strings for lockscreen shortcut hints -->
    <string name="left_shortcut_hint">Swipe right for %1$s</string>
    <string name="right_shortcut_hint">Swipe left for %1$s</string>
    <string name="lockscreen_message">Tap an icon on the left or right to reassign a lock screen shortcut.</string>
    <string name="lockscreen_default_target">Default</string>
    <string name="select_application">Select application</string>
    <string name="lockscreen_choose_action_title">Choose action</string>
    <string name="lockscreen_none_target">None</string>

    <!-- Content description of the data connection type HSPA+ for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_data_connection_hspap">HSPA+</string>

    <!-- Color for the RAM bar text in the recent view -->
    <color name="ram_bar_text">#FFFFFFFF</color>

    <!-- Task Manager -->
    <color name="memory_usage_item_background">#00000000</color>
    <color name="task_manager_title_text">#ffffffff</color>
    <color name="memory_usage_text_text">#ffffffff</color>
    <color name="qs_panel_background">#0000</color>
    <color name="task_item_view_background">#00000000</color>
    <color name="task_item_background">#0000</color>
    <color name="task_item_text">#ffffffff</color>

    <!-- Screenshot Delete Toast -->
    <string name="delete_screenshot_toast">Screenshot Deleted</string>

    <!-- SystemUI Tuner Parts -->
    <string name="systemui_tuner_statusbar_title">Statusbar Items</string>
    <string name="systemui_tuner_qs_title">Quick Settings</string>

    <!-- Text to use when the number in a notification info is too large
         (greater than status_bar_notification_info_maxnum, defined in
         values/config.xml) and must be truncated.
         [CHAR LIMIT=4] -->
    <string name="status_bar_notification_info_overflow" translatable="false">\u221E</string>

</resources>
